    </servlet>
    <servlet>
        <servlet-name>RegisterControllerMed</servlet-name>
        <servlet-class>com.mvc.controller.RegisterControllerMed</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>RegisterControllerPerson</servlet-name>
        <url-pattern>/RegisterControllerPerson</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LoginController</servlet-name>
        <url-pattern>/LoginController</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>BookingSwab</servlet-name>
        <url-pattern>/BookingSwab</url-pattern>
    </servlet-mapping>
        <servlet-mapping>
        <servlet-name>UpdateTampone</servlet-name>
        <url-pattern>/UpdateTampone</url-pattern>
    </servlet-mapping>
	<servlet-mapping>
        <servlet-name>Reports</servlet-name>
        <url-pattern>/Reports</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>RegisterControllerMed</servlet-name>
        <url-pattern>/RegisterControllerMed</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
