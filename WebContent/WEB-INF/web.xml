    <servlet>
        <servlet-name>LoginController</servlet-name>
        <servlet-class>com.mvc.controller.LoginController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>BookingSwab</servlet-name>
        <servlet-class>CommandPattern.BookingSwab</servlet-class>
    </servlet>   
    <servlet>
        <servlet-name>UpdateTampone</servlet-name>
        <servlet-class>StatePattern.UpdateTampone</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Reports</servlet-name>
